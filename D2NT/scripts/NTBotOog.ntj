Include('NTOog.ntj');
// Simplest class based oog example.
oog = new NTOogManager();
gm = oog.gm;

function NTMain() {
  Delay(5000);
  var _location;
  while (1) {
    Delay(1000);
    _location = controlData.getLocation();
    if (_location) gm.SetStatus(_location.name);
    oog.HandleMyLocation();
  }
}
