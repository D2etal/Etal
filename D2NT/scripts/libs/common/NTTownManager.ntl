var _NTTMGR_DoRevive;
var _NTTMGR_RemoveCurse;
var _NTTMGR_Simple;
var _NTTMGR_BuyArrows = false;
var ammyreq = Math.abs(Math.floor(me.GetStat(12) / 2)-90) * 2;
