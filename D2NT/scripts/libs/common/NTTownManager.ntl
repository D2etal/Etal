		NTU_MercDeathLog();
		//return NTTMGR_VisitTown();
		return true;
	}
	else if((selfmask.indexOf("01") != -1) && NTT_NeedPots()) {
		Print("I need some more potions!");
		//return NTTMGR_VisitTown();
		return true;
	}
	else if(NTTMGR_CheckCurse(selfmask, mercmask)) { 
		//return NTTMGR_VisitTown();
		return true;
	} else {
		return false;
	}
}

