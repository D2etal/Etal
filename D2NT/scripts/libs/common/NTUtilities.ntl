			if(Merc.GetStat(79) > 0) { MercStatMgr += COLOR_4 + "% Gold Find: " + COLOR_8 + Merc.GetStat(79) + "\n"; }
			if(Merc.GetStat(142 || 143 || 144 || 145 || 146 || 147 || 148 || 149) > 0) { MercStatMgr += COLOR_4 + "Absorbs are listed as 'Percent(Static)'" + "\n"; }
